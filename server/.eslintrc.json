{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
      "Atomics": "readable",
      "SharedArrayBuffer": "readable"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "arrow-parens": "off",
      "indent": ["error", 2]
  }
}